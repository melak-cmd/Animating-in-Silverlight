<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  SOLID COLOR BRUSH  -->
    <SolidColorBrush x:Key="AquaSolidBrush" Color="#FF00F6FF" />

    <!--  GRADIENT COLOR BRUSH  -->
    <RadialGradientBrush x:Key="BlueGradientBrush" GradientOrigin="-0.064,1.106" RadiusX="0.678" RadiusY="0.678">
        <GradientStop Offset="0" Color="#FF178AA1" />
        <GradientStop Offset="1" Color="#FF00DFFF" />
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="OrangeGradientBrush" GradientOrigin="-0.064,1.106" RadiusX="0.678" RadiusY="0.678">
        <GradientStop Offset="0" Color="#FFA19017" />
        <GradientStop Offset="1" Color="#FFE7B600" />
    </RadialGradientBrush>
    <SolidColorBrush x:Key="GreyBrush" Color="#FF575757" />
    <!--  Resource dictionary entries should be defined here.  -->
</ResourceDictionary>